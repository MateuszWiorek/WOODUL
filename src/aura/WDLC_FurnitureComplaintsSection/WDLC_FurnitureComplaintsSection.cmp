<aura:component description="WDLC_FurnitureComplaintsSection" controller="WDLC_FurnitureComplaintsController"
                implements="forceCommunity:availableForAllPageTypes">
    <aura:attribute name="complaints" type="Object[]" access="global"/>

    <aura:handler name="init" value="{!this}" action="{!c.onInit}"/>

    <aura:if isTrue="{!not(empty(v.complaints))}">
            <aura:iteration items="{!v.complaints}" var="comp">
                <c:WDLC_OrderComplaintRpw complaint="{!comp}"/>
            </aura:iteration>
    </aura:if>
</aura:component>
